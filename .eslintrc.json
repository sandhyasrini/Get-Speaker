{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard-with-typescript"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"] // Your TypeScript files extension
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json",
    "emitWarning": true
  },
  "plugins": ["react"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["warn"]

  }
}
